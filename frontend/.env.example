# Configurações da API
VITE_API_URL=http://localhost:4000
VITE_CLIENT_TARGET=http://localhost:4000

# Configurações de Autenticação
VITE_USE_MOCK_AUTH=true
VITE_JWT_SECRET=uma-chave-segura-minimo-32-caracteres-para-jwt-tokens-desenvolvimento

# Configurações do Frontend
VITE_FRONTEND_URL=http://localhost:5173
VITE_APP_NAME=CRM Multi-Acesso
VITE_APP_VERSION=1.0.0

# Configurações de Debug
VITE_DEBUG=true
VITE_LOG_LEVEL=info

# Configurações de Integração
VITE_MICROSOFT_CLIENT_ID=your_microsoft_client_id_here
VITE_MICROSOFT_REDIRECT_URI=http://localhost:4000/oauth/callback

# Configurações de Timeout
VITE_API_TIMEOUT=10000
VITE_REQUEST_TIMEOUT=5000